<h1>All Projects</h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Start Date</th>
      <th>End Date</th>
      <th>Status</th>
    </tr>
  </thead>
  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= project.name %></td>
        <td><%= project.description %></td>
        <td><%= project.start_date.strftime("%m/%d/%Y") %></td>
        <td><%= project.end_date.strftime("%m/%d/%Y") %></td>
        <td><%= project.status %></td>
        <td><%= button_to "Delete",  project_path(project), method: :delete, data: { confirm: "Are you sure?" } %></td>

      </tr>
    <% end %>
  </tbody>
</table>
